<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mypage_background2"
    tools:context="Mypage.MypageFragment">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.31" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="4dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.89" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="320dp"
        android:layout_height="30dp"
        android:fontFamily="@font/cafe24ohsquareair"
        android:text="회원정보"
        android:textColor="#151414"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline4" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.72" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="320dp"
        android:layout_height="30dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/cafe24ohsquareair"
        android:text="고객센터"
        android:textColor="#151414"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/btn_mp_delete"
        app:layout_constraintTop_toTopOf="@+id/guideline7" />

    <Button
        android:id="@+id/btn_mp_inquriy"
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:layout_marginTop="5dp"
        android:backgroundTint="#1B3039"
        android:drawableLeft="@android:drawable/ic_menu_send"
        android:fontFamily="@font/cafe24ohsquareair"
        android:text="문의하기"
        android:textColor="#EAEAEA"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Button
        android:id="@+id/btn_mp_member"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginTop="5dp"
        android:backgroundTint="#1B3039"
        android:fontFamily="@font/cafe24ohsquareair"
        android:text="회원정보"
        android:textColor="#EAEAEA"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/btn_mp_update"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginTop="5dp"
        android:backgroundTint="#1B3039"
        android:fontFamily="@font/cafe24ohsquareair"
        android:text="회원수정"
        android:textColor="#EAEAEA"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_mp_member"
        app:layout_constraintStart_toStartOf="@+id/btn_mp_member"
        app:layout_constraintTop_toBottomOf="@+id/btn_mp_member" />

    <Button
        android:id="@+id/btn_mp_delete"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginTop="5dp"
        android:backgroundTint="#1B3039"
        android:fontFamily="@font/cafe24ohsquareair"
        android:text="회원탈퇴"
        android:textColor="#EAEAEA"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/btn_mp_update"
        app:layout_constraintStart_toStartOf="@+id/btn_mp_update"
        app:layout_constraintTop_toBottomOf="@+id/btn_mp_update" />

    <TextView
        android:id="@+id/txt_mypage_nick"
        android:layout_width="300dp"
        android:layout_height="45dp"
        android:layout_marginTop="112dp"
        android:fontFamily="@font/cafe24ohsquareair"
        android:gravity="center"
        android:textColor="@color/bluegray"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline5"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>